      "subPath": {
        "Total copies": "TotalImpressions.#text",
        "Total copies from ADF": "AdfImages",
        "Total double-sided pages scanned": "DuplexSheets.#text",
        "Total pages from scanner glass": "FlatbedImages",
        "Total black-and-white copies": "MonochromeImpressions",
        "Total color copies": "ColorImpressions"
      }
