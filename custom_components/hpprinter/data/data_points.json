        "Genuine Refilled Count": "RefilledCount.GenuineRefilledCount"
      }
    },
    {
      "name": "Printer",
      "endpoint": "/DevMgmt/ProductUsageDyn.xml",
      "path": "ProductUsageDyn.PrinterSubunit",
      "subPath": {
        "Total pages printed": "TotalImpressions.#text",
        "Total black-and-white pages printed": "MonochromeImpressions",
        "Total color pages printed": "ColorImpressions",
        "Total single-sided pages printed": "SimplexSheets",
        "Total double-sided pages printed": "DuplexSheets.#text",
        "Total jams": "JamEvents.#text",
        "Total miss picks": "MispickEvents"
      }
    },
    {
      "name": "Scanner",
      "endpoint": "/DevMgmt/ProductUsageDyn.xml",
      "path": "ProductUsageDyn.ScannerEngineSubunit",
      "subPath": {
        "Total scanned pages": "ScanImages.#text",
