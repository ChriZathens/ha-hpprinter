        "Connection Method": "ConnectionMethod"
      }
    },
    {
      "name": "Product",
      "endpoint": "/DevMgmt/ProductConfigDyn.xml",
      "path": "ProductConfigDyn.ProductInformation",
      "subPath": {
        "Make And Model": "MakeAndModel",
        "Make And Model Family": "MakeAndModelFamily",
        "SKU Identifier": "SKUIdentifier",
        "Serial Number": "SerialNumber",
        "Product Number": "ProductNumber",
