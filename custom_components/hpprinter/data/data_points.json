        "DeviceConnectivityPortType": "HardwareConfig.DeviceConnectivityPortType",
        "IsConnected": "HardwareConfig.IsConnected"
      }
    },
    {
      "name": "ePrint",
      "endpoint": "/ePrint/ePrintConfigDyn.xml",
      "path": "ePrintConfigDyn",
      "subPath": {
